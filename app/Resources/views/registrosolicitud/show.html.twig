{% extends 'base.html.twig' %}

{% block body %}
    <h1>Registrosolicitud</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ registroSolicitud.id }}</td>
            </tr>
            <tr>
                <th>Centromedico</th>
                <td>{{ registroSolicitud.centroMedico }}</td>
            </tr>
            <tr>
                <th>Tratamiento</th>
                <td>{{ registroSolicitud.tratamiento }}</td>
            </tr>
            <tr>
                <th>Montosolicitado</th>
                <td>{{ registroSolicitud.montoSolicitado }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ registroSolicitud.observacion }}</td>
            </tr>
            <tr>
                <th>Informemedicoruta</th>
                <td>{{ registroSolicitud.informeMedicoRuta }}</td>
            </tr>
            <tr>
                <th>Presupuestoruta</th>
                <td>{{ registroSolicitud.presupuestoRuta }}</td>
            </tr>
            <tr>
                <th>Fechaaprobado</th>
                <td>{% if registroSolicitud.fechaAprobado %}{{ registroSolicitud.fechaAprobado|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Completado</th>
                <td>{% if registroSolicitud.completado %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('registrosolicitud_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('registrosolicitud_edit', { 'id': registroSolicitud.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
